{
  "routes": [
    {
      "route": "/secure",
      "allowedRoles": [ "admin" ]
    },
    {
      "route": "/callback",
      "allowedRoles": [ "admin" ]
    },
    {
      "route": "/api/*",
      "methods": [ "GET" ],
      "allowedRoles": [ "admin" ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}